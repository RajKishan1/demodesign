generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model testimonial {
  id         Int       @id @default(autoincrement())
  name       String
  review     String?
  rating     Decimal?  @db.Decimal(5, 1)
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
